<?xml version="1.0"?>
<!DOCTYPE resource-action-mapping PUBLIC "-//Liferay//DTD Resource Action Mapping 7.4.0//EN" "http://www.liferay.com/dtd/liferay-resource-action-mapping_7_4_0.dtd">

<resource-action-mapping>
    <model-resource>
        <model-name>com.example.reserved.column.filter.demo.foo</model-name>
        <portlet-ref>
            <portlet-name>com_example_reserved_column_filter_demo_portlet_FooPortlet</portlet-name>
        </portlet-ref>
        <root>true</root>
        <weight>1</weight>
        <permissions>
            <supports>
                <action-key>ADD_FOO</action-key>
                <action-key>PERMISSIONS</action-key>
                <action-key>UPDATE</action-key>
                <action-key>VIEW</action-key>
            </supports>
            <site-member-defaults>
            </site-member-defaults>
            <guest-defaults />
            <guest-unsupported>
                <action-key>ADD_FOO</action-key>
                <action-key>PERMISSIONS</action-key>
                <action-key>UPDATE</action-key>
                <action-key>VIEW</action-key>
            </guest-unsupported>
        </permissions>
    </model-resource>

    <model-resource>
        <model-name>com.example.reserved.column.filter.demo.model.Foo</model-name>
        <portlet-ref>
            <portlet-name>com_example_reserved_column_filter_demo_portlet_FooPortlet</portlet-name>
        </portlet-ref>
        <weight>2</weight>
        <permissions>
            <supports>
                <action-key>DELETE</action-key>
                <action-key>PERMISSIONS</action-key>
                <action-key>UPDATE</action-key>
                <action-key>VIEW</action-key>
            </supports>
            <site-member-defaults>
            </site-member-defaults>
            <guest-defaults/>
            <guest-unsupported>
                <action-key>DELETE</action-key>
                <action-key>PERMISSIONS</action-key>
                <action-key>UPDATE</action-key>
                <action-key>VIEW</action-key>
            </guest-unsupported>
        </permissions>
    </model-resource>
</resource-action-mapping>